<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="all" package="org.sakaiproject.articulate.tincan.model.hibernate" default-lazy="false">

    <class name="org.sakaiproject.articulate.tincan.model.hibernate.ArticulateTCContentPackage" table="ARTICULATE_TC_CONTENT_PACKAGE">
        <id name="contentPackageId" column="ID" type="long">
            <generator class="native">
                <param name="sequence">ARTICULATE_TC__UID_S</param>
            </generator>
        </id>
        <property name="title" column="TITLE" />
        <property name="resourceId" column="RESOURCE_ID" />
        <property name="context" column="CONTEXT" />
        <property name="url" column="URL" />
        <property name="recordType" column="RECORD_TYPE" />
        <property name="releaseOn" column="RELEASE_ON" />
        <property name="dueOn" column="DUE_ON" />
        <property name="acceptUntil" column="ACCEPT_UNTIL" />
        <property name="createdOn" column="CREATED_ON" />
        <property name="createdBy" column="CREATED_BY" />
        <property name="modifiedOn" column="MODIFIED_ON" />
        <property name="modifiedBy" column="MODIFIED_BY" />
        <property name="numberOfTries" column="NUMBER_OF_TRIES" />
        <property name="deleted" column="IS_DELETED" />
        <property name="assignmentId" column="ASSIGNMENT_ID" />
    </class>

</hibernate-mapping>
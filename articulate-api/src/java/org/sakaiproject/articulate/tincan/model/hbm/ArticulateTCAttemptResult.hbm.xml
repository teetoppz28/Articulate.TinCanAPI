<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-cascade="all" package="org.sakaiproject.articulate.tincan.model.hibernate" default-lazy="false">

    <class name="org.sakaiproject.articulate.tincan.model.hibernate.ArticulateTCAttemptResult" table="ARTICULATE_TC_ATTEMPT_RESULT">
        <id name="id" column="ID" type="long">
            <generator class="increment"/>
        </id>
        <properties name="attempt_result_unique" unique="true">
            <property name="attemptId" column="ATTEMPT_ID" />
            <property name="attemptNumber" column="ATTEMPT_NUMBER" />
        </properties>
        <property name="scaledScore" column="SCALED_SCORE" />
        <property name="dateCompleted" column="DATE_COMPLETED" />
        <property name="modified" column="MODIFIED" />
    </class>

</hibernate-mapping>
